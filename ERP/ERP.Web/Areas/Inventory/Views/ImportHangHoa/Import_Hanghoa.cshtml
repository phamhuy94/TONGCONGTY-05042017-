                    <br />

                    @using (Html.BeginForm("Import_Kho", "ImportHangHoa", FormMethod.Post, new { enctype = "multipart/form-data" }))
                    {
                        <h1>Inventory</h1>

                        <table>
                            <tr>
                                <th><h1>Employee</h1></th>
                                <th>
                                    <a href="~/Content/Files/IMPORT_KHO.xlsx" class="fa fa-file-excel-o"> Get sample file</a>
                                </th>
                            </tr>
                            <tr>
