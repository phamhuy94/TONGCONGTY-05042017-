    <!-- Modal HÃNG-->
    <div class="modal fade" id="hangsp" role="dialog">
        <div class="modal-dialog modal-lg">
            <div class="modal-content">
                <div class="modal-body">
                    <br />







                    @using (Html.BeginForm("Import_Hangsp", "ImportHangHoa", FormMethod.Post, new { enctype = "multipart/form-data" }))
                    {


                        <table>
                            <tr>
                                <th><h1>Maker</h1></th>
                                <th>
                                    <a href="~/Content/Files/IMPORT_MAKER.xlsx" class="fa fa-file-excel-o"> Get sample file</a>
                                </th>
                            </tr>
                            <tr>
                                <td colspan="2">
                                    <hr />
                                    <input type="file" name="UploadedFile" class="file" value="Choose File From Your Computer">
