
@{
    ViewBag.Title = " Import File";
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>HOPLONG | Trang chủ</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <!-- Bootstrap 3.3.6 -->
    <link href="~/Content/download/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
    <link href='http://fonts.googleapis.com/css?family=Roboto:100,400,300,500,700' rel='stylesheet' type='text/css'>
    <link href="~/Content/download/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
    <link href="//maxcdn.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.min.css" rel="stylesheet">
    <link href='http://fonts.googleapis.com/css?family=Shadows+Into+Light' rel='stylesheet' type='text/css'>
    <link href="~/Content/self-writing/css/Layout-CSS.css" rel="stylesheet" />
    <link href="~/Content/self-writing/css/ImportFile.css" rel="stylesheet" />

</head>
<body class="hold-transition skin-blue sidebar-mini">

    <div class="content">
        <div class="card">
            <div class="container-fliud" style="padding-top: 2%">
                <center><b>You're in the area where help you import data using excel</b> </center>
                <div class="wrapper row">
                    <div class="preview col-md-5">
                        <div class="preview-pic tab-content">
                            <img src="~/Content/self-writing/Images/hoplong.jpg" style="padding-top:10%" width="660" height="600" />
                        </div>
                    </div>
                    <div class="details col-md-5">
                        <h3 class="product-title">Description</h3>
                        <p class="product-description">Chọn mục mà bạn muốn nhập khẩu dữ liệu bằng excel tương ứng với các mục bên dưới.</p>
                        <table>
                            <tr>
                                <td><p class="product-description"><b>Maker stock:</b>Tồn kho hãng.</p></td>
                            </tr>
                            <tr>
                                <td><p class="product-description"><b>Stock:</b> Tồn kho nội bộ.</p></td>
                            </tr>
                            <tr>
                                <td><p class="product-description"><b>Inventory:</b> Danh sách kho.</p></td>
                            </tr>
                            <tr>
                                <td><p class="product-description"><b>Maker:</b> Hãng hàng hóa.</p></td>
                            </tr>
                        </table>



                        <br />
                        <table style="width: 100%;text-align: center;">
                            <tr>
                                <td style="padding-top: 20px;">
                                    <center>
                                        <!--Button hàng hóa-->
                                        <div class="style_prevu_kit" style="background-color:#5BC0DE;">
                                            <button type="button" class="btn btn-info btn-lg" data-toggle="modal" data-target="#myModal" style="border: none; border-radius:16px"><i class="fa fa-gift fa-3x"></i></button>
                                        </div>
                                        <!------------------------------------------------------------>
                                    </center>
                                </td>
                                <td>
                                    <center>
                                        <!--Button hàng tồn kho-->
                                        <div class="style_prevu_kit" style="background-color:#5BC0DE;">
                                            <button type="button" class="btn btn-info btn-lg" data-toggle="modal" data-target="#hangtonkho" style="border: none; border-radius:16px"><i class="fa fa-database fa-3x"></i></button>
                                        </div>
                                        <!------------------------------------------------------------>
                                    </center>
                                </td>
                            </tr>
                            <tr style="border-top: 0px">
                                <td style="padding-top: 20px;padding-bottom: 20px">Product</td>
                                <td style="padding-top: 20px;padding-bottom: 20px">Stock</td>
                            </tr>
                            <tr>
                                <td>
                                    <center>
                                        <!--Button KHO-->
                                        <div class="style_prevu_kit" style="background-color:#5BC0DE;">
                                            <button type="button" class="btn btn-info btn-lg" data-toggle="modal" data-target="#kho" style="border: none; border-radius:16px"><i class="fa fa-home fa-3x"></i></button>
                                        </div>
                                        <!------------------------------------------------------------>
                                    </center>
                                </td>
                                <td>
                                    <center>
                                        <!--Button HÃNG SẢN PHẨM-->
                                        <div class="style_prevu_kit" style="background-color:#5BC0DE;">
                                            <button type="button" class="btn btn-info btn-lg" data-toggle="modal" data-target="#hangsp" style="border: none; border-radius:16px"><i class="fa fa-tag fa-3x"></i></button>
                                        </div>
                                        <!------------------------------------------------------------>
                                    </center>
                                </td>
                            </tr>
                            <tr style="border-top: 0px">
                                <td style="padding-top: 20px;padding-bottom: 20px">Inventory</td>
                                <td style="padding-top: 20px;padding-bottom: 20px">Maker</td>
                            </tr>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>


    <!------------------------------------------------------------>
    <!-- Modal HANG HOA-->
    <div class="modal fade" id="myModal" role="dialog">
        <div class="modal-dialog modal-lg">
            <div class="modal-content">
                <div class="modal-body">
                    <table>


                        <tr>

                            <td>
                                <h1>Stock</h1>

                            </td>
                            <td>
                                <a href="~/Content/Files/IMPORT_HANGTONKHO.xlsx" class="fa fa-file-excel-o"> Get sample file</a>

                            </td>
                        </tr>
                        <tr>
                            <!-- Form Name -->

                            <td class="col-md-6">
                                @using (Html.BeginForm("Import_Hanghoa", "ImportHangHoa", FormMethod.Post, new { enctype = "multipart/form-data" }))
                                {

                                    <h3>New import</h3>
                                    <hr />
                                    <input type="file" name="UploadedFile" class="file" value="Choose File From Your Computer">
                                    <br />
                                    <input type="submit" class="btn btn-success" value="Add" />

                                }
                            </td>
                            <td class="col-md-6">
                                @using (Html.BeginForm("Update_Hanghoa", "ImportHangHoa", FormMethod.Post, new { enctype = "multipart/form-data" }))
                                {

                                    <h3>Update</h3>
                                    <hr />
                                    <input type="file" name="UploadedFile" class="file" value="Choose File From Your Computer">
                                    <br />
                                    <input type="submit" class="btn btn-success" value="Update" />

                                }
                            </td>

                        </tr>
                        <tr>
                            <td style="padding-top: 12px; padding-left:12px">
                                Dữ liệu trên tệp mà chưa có trong hệ thống thì sẽ được thêm vào, các dữ liệu khác sẽ giữ nguyên
                            </td>
                            <td style="padding-left:12px">
                                Toàn bộ dữ liệu trên tệp sẽ được cập nhật trên hệ thống
                            </td>
                        </tr>

                        <tr>
                            <td colspan="2">
                                <p style="color:blue">@ViewBag.Message</p>
                                <p style="color:red">@ViewBag.Dongloi</p>

                                <p style="color:blue">@ViewBag.Information</p>
                                <p style="color:red">@ViewBag.Error</p>
                            </td>
                        </tr>
                    </table>
                 
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-danger" data-dismiss="modal">Exit</button>
                </div>
            </div>
        </div>
    </div>
    <!--END MODAL-->
    <br />
    <!-- Modal Tồn kho hãng-->
    <div class="modal fade" id="modaltonkhohang" role="dialog">
        <div class="modal-dialog modal-lg">
            <div class="modal-content">
                <div class="modal-body">

                    <br />
                    @using (Html.BeginForm("Import_TonKhoHang", "ImportHangHoa", FormMethod.Post, new { enctype = "multipart/form-data" }))
                    {

                            <!-- Form Name -->


                        <table>
                            <tr>
                                <th><h1>Maker Stock</h1></th>
                                <th>
                                    <a href="~/Content/Files/IMPORT_TONKHO_HANG.xlsx" class="fa fa-file-excel-o"> Get sample file</a>
                                </th>
                            </tr>
                            <tr>
                                <td colspan="2">
                                    <hr />
                                    <input type="file" name="UploadedFile" class="file" value="Choose File From Your Computer">
                                    <br />
                                    <input type="submit" class="btn btn-success" value="Add" />

                                </td>

                            </tr>
                            <tr>
                                <td colspan="2">Dữ liệu trên tệp mà chưa có trong hệ thống thì sẽ được thêm vào, các dữ liệu khác sẽ giữ nguyên</td>

                            </tr>
                            <tr>
                                <td colspan="2">
                                    <p style="color:blue">@ViewBag.Message</p>
                                    <p style="color:blue">@ViewBag.Information</p>
                                    <p style="color:red">@ViewBag.Error</p>
                                </td>

                            </tr>
                        </table>


                    }

                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-danger" data-dismiss="modal">Exit</button>
                </div>
            </div>
        </div>
    </div>
    <!--END MODAL-->
    <br />
    <!-- Modal HÀNG TỒN KHO-->
    <div class="modal fade" id="hangtonkho" role="dialog">
        <div class="modal-dialog modal-lg">
            <div class="modal-content">
                <div class="modal-body">

                    <br />
                    <table>


                        <tr>

                            <td>
                                <h1>Stock</h1>

                            </td>
