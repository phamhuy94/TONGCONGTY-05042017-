
@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Inventory/Views/Shared/Layout.cshtml";
}



<meta charset="utf-8">
<meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
<link href="~/Content/self-writing/css/HangHoaHL.css" rel="stylesheet" />
<script src="http://malsup.github.com/jquery.form.js"></script>

<div class="panel-heading">Bạn đang xem danh mục hàng hóa</div>
<br />
<div ng-app="myApp" ng-controller="hanghoaCtrl">
    <div class="content">
        
        <button type="button" class="btn btn-info" data-toggle="modal" data-target="#myModal">Add New Product</button>
        <br />
        <br />
        <!-- Modal Thêm mới -->
        <div class="modal fade" id="myModal" role="dialog" tabindex="-1">
            <div class="modal-dialog modal-lg" style="width: 90%">
                <div class="modal-content" style="height: 100%">

                    <div class="modal-body">
                            <fieldset style="height: 100%">
                                <!-- Form Name -->
                                <legend>Thêm Hàng Mới</legend>
                                <!-- Text input-->
                                <div class="col-md-6 container_left">
                                    <div class="col-md-11 content_left">
                                        <div class="form-group">
                                            <div class="col-md-12">
                                                <input type="text" placeholder="Mã hàng" class="form-control input-md" ng-model="mahang">
                                            </div>                                           
                                        </div>
                                        <div class="form-group">
                                            <div class="col-md-12">
                                                <input type="text" placeholder="Mã chuẩn" class="form-control input-md" ng-model="machuan">
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="col-md-12">
                                                <input type="text" placeholder="Mã nhập hàng" class="form-control input-md" ng-model="manhaphang">
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="col-md-12">
                                                <input type="text" placeholder="Mã chuyển đổi" class="form-control input-md" ng-model="machuyendoi">
                                            </div>
                                        </div>
                                        <!-- Text input-->
                                        <div class="form-group">
                                            <div class="col-md-12">
                                                <input type="text" placeholder="Tên hàng" class="form-control input-md" ng-model="tenhang">
                                            </div>

                                        </div>
                                        <!-- Text input-->
                                        <div class="form-group">
                                            <div class="col-md-12">
                                                <select ng-model="manhomhang" class="form-control input-md">
                                                    <option ng-selected="selectedValue == abc.MA_NHOM_HANG_CHI_TIET" ng-repeat="abc in danhsachnhomhang" value="{{abc.MA_NHOM_HANG_CHI_TIET}}">{{abc.CHUNG_LOAI_HANG}}</option>
                                                </select>
                                            </div>
                                        </div>
                                        <!-- Text input-->
                                        <div class="form-group">
                                            <div class="col-md-12">
                                                <input type="text" placeholder="Khối lượng" class="form-control input-md" ng-model="khoiluong">
                                            </div>
                                        </div>
                                        <!-- Text input-->
                                        <div class="form-group">
                                            <div class="col-md-12">
                                                <input type="text" placeholder="Đơn vị tính" class="form-control input-md" ng-model="donvitinh">
                                            </div>
                                        </div>
                                        <!-- Text input-->
                                        <div class="form-group">
                                            <div class="col-md-12">
                                                <input type="text" placeholder="Bảo hành" class="form-control input-md" ng-model="baohanh">
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="col-md-12">
                                                <input type="text" placeholder="Giá nhập" class="form-control input-md" ng-model="gianhap">
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="col-md-12">
                                                <input type="text" placeholder="Giá list" class="form-control input-md" ng-model="gialist">
                                            </div>
                                        </div>
                                        <!-- Text input-->
                                        <div class="form-group">
                                            <div class="col-md-12">
                                                <input type="text" placeholder="Xuất xứ" class="form-control input-md" ng-model="xuatxu">
                                            </div>
                                        </div>
                                        <!-- Text input-->
                                        <div class="form-group">
                                            <div class="col-md-12">
                                                <input type="text" placeholder="Tài khoản hạch toán kho" class="form-control input-md" ng-model="tkhachtoankho">
                                            </div>
                                        </div>
                                        <!-- Text input-->
                                        <div class="form-group">
                                            <div class="col-md-12">
                                                <input type="text" placeholder="Tài khoản doanh thu" class="form-control input-md" ng-model="tkdoanhthu">
                                            </div>
                                        </div>
                                        <!-- Text input-->
                                        <div class="form-group">
                                            <div class="col-md-12">
                                                <input type="text" placeholder="Tài khoản chi phí" class="form-control input-md" ng-model="tkchiphi">
                                            </div>
                                        </div>

                                        <div class="form-group">
                                            <div class="col-md-12">
                                                <textarea id="ghichu" name="ghichu" placeholder="Ghi chú" class="form-control input-md" ng-model="ghichu"></textarea>
                                            </div>
                                        </div>
                                        <div class="form-group" style="margin-top: 10%">
                                            <form action="/Inventory/HangHoa" ng-controller="imgCtrl" method="post">
                                                <div class="col-md-3">
                                                    @Html.AntiForgeryToken()
                                                    <input type='file' name="files" id="imgInp" style="width: 100%" />
                                                    <input type="submit" value="Upload Ảnh hàng hóa" style="margin-top: 30px;" />
                                                </div>
                                                <div class="col-md-9">
                                                    <img id="blah" src="#" height="250" width="250" />
                                                </div>
                                            </form>
                                        </div>
                                    </div>
                                    <div style="padding-top:8px;" class="col-md-1">
                                        <i style="font-size:24px;" class="fa fa-question-circle nohelp_left" aria-hidden="true" onclick="help_left()"></i>
                                        <i style="font-size:24px;display: none" class="fa fa-ban help_left" aria-hidden="true" onclick="nohelp_left()"></i>
                                    </div>
                                </div>

                                <div class="col-md-4 help_left">
                                    <div class="form-group">
                                        <ul class="callouts">
                                            <li style="width:500px;height:50px;" class="callouts--left">Mã hàng: Mã số hàng hóa duy nhất trong hệ thống của Hợp Long </li>
                                        </ul>
                                    </div>
                                    <div class="form-group">
                                        <ul class="callouts">
                                            <li style="width:500px;height:50px;" callouts--left">Mã chuẩn: Mã số chuẩn theo bảng giá list của hãng </li>
                                        </ul>
                                    </div>
                                    <div class="form-group">
                                        <ul class="callouts">
                                            <li style="width:500px;height:50px;" callouts--left">Mã nhập hàng: Mã dùng cho phòng mua đặt hàng với nhà cung cấp</li>
                                        </ul>
                                    </div>
                                    <div class="form-group">
                                        <ul class="callouts">
                                            <li style="width:500px;height:80px;" callouts--left">Mã chuyển đổi: Dùng trong trường hợp mã chuẩn đã ngưng sản xuất và chuyển sang mã mới </li>
                                        </ul>
                                    </div>
                                    <div class="form-group" style="margin-top :10%">
                                        <ul class="callouts">
                                            <li style="width:500px;height:50px;" callouts--left">Tên hàng: Tên của hàng hóa </li>
                                        </ul>
                                    </div>
                                    <div class="form-group">
                                        <ul class="callouts">
                                            <li style="width:500px;height:50px;" callouts--left">Hãng sản xuất của mã hàng </li>
                                        </ul>
                                    </div>
                                    <div class="form-group">
                                        <ul class="callouts">
                                            <li style="width:500px;height:50px;" callouts--left">Khối lượng của hàng hóa(tính bằng g)</li>
                                        </ul>
                                    </div>
                                    <div class="form-group">
                                        <ul class="callouts">
                                            <li style="width:500px;height:50px;" callouts--left">Đơn vị tính (cái, chiếc,...)</li>
                                        </ul>
                                    </div>
                                    <div class="form-group">
                                        <ul class="callouts">
                                            <li style="width:500px;height:80px;" callouts--left">Bảo hành: Thời gian bảo hành của hàng hóa (tính bằng tháng),nhập vào theo dạng số (12, 24, ...) </li>
                                        </ul>
                                    </div>
                                    <div class="form-group" style="margin-top :10%">
                                        <ul class="callouts">
                                            <li style="width:500px;height:50px;" callouts--left">Giá nhập: Giá nhập vào theo bảng giá của hãng</li>
                                        </ul>
                                    </div>
                                    <div class="form-group">
                                        <ul class="callouts">
                                            <li style="width:500px;height:50px;" callouts--left">Giá list: Giá theo Price List của Hãng gửi về</li>
                                        </ul>
                                    </div>
                                    <div class="form-group">
                                        <ul class="callouts">
                                            <li style="width:500px;height:90px;" callouts--left">Xuất xứ: JPN: Nhật Bản, CHN: Trung Quốc, ...</li>
                                        </ul>
                                    </div>
                                    <div class="form-group" style="margin-top :10%">
                                        <ul class="callouts">
                                            <li style="width:500px;height:90px;" callouts--left">Tài khoản hạch toán kho: chỉ dùng cho kế toán(tài khoản kho hàng hóa: 1561) </li>
                                        </ul>
                                    </div>
                                    <div class="form-group" style="margin-top :10%">
                                        <ul class="callouts">
                                            <li style="width:500px;height:90px;" callouts--left">Tài khoản doanh thu: chỉ dùng cho kế toán(tài khoản: 51111) </li>
                                        </ul>
                                    </div>
                                    <div class="form-group" style="margin-top :10%">
                                        <ul class="callouts">
                                            <li style="width:500px;height:90px;" callouts--left">Tài khoản chi phí: chỉ dùng cho kế toán(tài khoản: 632) </li>
                                        </ul>
                                    </div>
                                </div>

                                <div class="col-md-6 container_right">
                                    <div class="col-md-12 content_right"> 
                                        <div class="form-group">
                                            <div class="col-md-12">
                                                <textarea id="ghichu" name="ghichu" placeholder="Thông số của hàng hóa (Nếu có)" class="form-control input-md" ng-model="thongso"></textarea>
                                            </div>
                                        </div>  
                                        <div class="form-group"  style="margin-top :10%">
                                            <label class="col-md-4 control-label" style="padding-top: 0px !important" for="name">Hàng đã ngưng sản xuất:  </label>
                                            <div class="col-md-8">
                                                <input type="checkbox" ng-model="discontinue">
                                            </div>
                                        </div>    
                                                                                                                  
                                        <div class="form-group col-md-12" >
                                            <label class="control-label ckeditor" for="name">Cách thức đóng gói: </label>
                                            <div class="ckeditor">
                                                <textarea id="donggoi" name="donggoi" placeholder="Packing" class="form-control input-md" ng-model="quycachdonggoi"></textarea>
                                            </div>
                                            <label class="control-label ckeditor" for="name"> Thông số kỹ thuật khác: </label>
                                            <div class="ckeditor">
                                                <textarea id="thongso" name="thongso" placeholder="Specification" class="form-control input-md" ng-model="thongsokythuat"></textarea>
                                            </div>
                                        </div>
                                    </div>
                                </div>                                                                                                                            
                                <script>
                                    CKEDITOR.replace('donggoi', {
                                        height: 150
                                    });
                                    CKEDITOR.replace('thongso', {
                                        height: 150
                                    });

                                </script>

                            </fieldset>
                    </div>
                    <div class="modal-footer">
                        <div class="form-group">
                            <label class="col-md-4 control-label" for="anmelden"></label>
                            <div class="col-md-8 addnew">
                                <button id="addnew" name="addnew" class="btn addnewmodal" ng-click="add()" data-dismiss="modal">Lưu</button>
                                <button id="exit" name="exit" class="btn addnewmodal" data-dismiss="modal">Thoát</button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!--end modal-->
        <!-- Modal Update -->
        <div class="modal fade" id="myEdit" role="dialog" tabindex="-1">
            <div class="modal-dialog modal-lg" style="width: 90%">
                <div class="modal-content" style="height: 100%">

                    <div class="modal-body">
                            <fieldset style="height: 100%">
                                <!-- Form Name -->
                                <legend>Update Product</legend>
                                <!-- Text input-->
                                <div class="col-md-6 container_left" >
                                    <div class="col-md-11">
                                        <div class="form-group">
                                            <label class="col-md-3 control-label" for="name">Mã hàng: </label>
                                            <div class="col-md-9">
                                                <input type="text" placeholder="Mã hàng" class="form-control input-md" ng-model="item.MA_HANG">
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-3 control-label" for="name">Mã chuẩn: </label>
                                            <div class="col-md-9">
                                                <input type="text" placeholder="Mã chuẩn" class="form-control input-md" ng-model="item.MA_CHUAN">
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-3 control-label" for="name">Mã nhập hàng: </label>
