                                            <label class="col-md-3 control-label no-pad-top" for="name">Tài khoản kho </label>
                                            <div class="col-md-9">
                                                <input type="text" placeholder="Tài khoản kho" class="form-control input-md" ng-model="item.TK_HACH_TOAN_KHO">
                                            </div>
                                        </div>
                                        <!-- Text input-->
                                        <div class="form-group">
                                            <label class="col-md-3 control-label" for="name">Tài khoản doanh thu: </label>
